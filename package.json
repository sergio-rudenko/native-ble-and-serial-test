{
  "name": "skud-sprut-key-manager-client",
  "private": true,
  "version": "1.0.3",
  "type": "commonjs",
  "description": "Bastion SKUD SPRUT Key manager",
  "scripts": {
    "dev": "vite",
    "build": "eslint ./ && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./ --fix",
    "electron:start-live": "npm run build && npx cap sync @capacitor-community/electron && cd electron && npm run start-live",
    "electron:start": "npm run build && npx cap sync @capacitor-community/electron && cd electron && npm run start",
    "electron:pack": "npm run build && npx cap sync @capacitor-community/electron && cd electron && npm run pack",
    "electron:build:windows": "npm run build && npx cap sync @capacitor-community/electron && cd electron && npm run build:windows",
    "electron:build:linux": "npm run build && npx cap sync @capacitor-community/electron && cd electron && npm run build:linux"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/react": "^7.6.1",
    "@ionic/react-router": "^7.6.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "ionicons": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "5.5.1",
    "@types/dom-serial": "^1.0.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  }
}
